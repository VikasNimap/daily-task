class List{
    constructor(data,next){
        this.data = data,
        this.next = null
    }
}

class LinkedList{
    constructor(data,next){
        this.head = null,
        this.tail = null
    }
    insertAtBeginning(data){
        const list = new List();
        list.data = data
        if(!this.head){
            this.head = list,
            this.tail = list
        }
        else{
            list.next = this.head,
            this.head = list
        }
    }
}